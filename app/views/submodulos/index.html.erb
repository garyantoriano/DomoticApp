<h1>Listing submodulos</h1>

<table>
  <thead>
    <tr>
      <th>Submodulo</th>
      <th>Modelo</th>
      <th>Pinesdigitales</th>
      <th>Pinesanalogicos</th>
      <th>Sistema domotico</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submodulos.each do |submodulo| %>
      <tr>
        <td><%= submodulo.submodulo %></td>
        <td><%= submodulo.modelo %></td>
        <td><%= submodulo.pinesDigitales %></td>
        <td><%= submodulo.pinesAnalogicos %></td>
        <td><%= submodulo.sistema_domotico %></td>
        <td><%= link_to 'Show', submodulo %></td>
        <td><%= link_to 'Edit', edit_submodulo_path(submodulo) %></td>
        <td><%= link_to 'Destroy', submodulo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submodulo', new_submodulo_path %>
