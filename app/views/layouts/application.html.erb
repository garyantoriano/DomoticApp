<!DOCTYPE html>
<html>
<head>
  <title>DomoticApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
</head>
<body background="assets/Fondo7.jpg">

	<div class="row" style="margin-top:45px;">
		<div class="col-lg-offset-1 col-lg-10" id="layout_header">
			<%=render 'layouts/header'%>
		</div>
	</div>

	<div class="row">
		<div class="col-lg-offset-1 col-lg-3"  id="layout_sidebar">
			<%=render 'layouts/sidebar'%>
		</div>
			
		<div class="col-lg-7" id="layout_center">
			<%= yield %>
		</div>
	</div>

	<div class="row">
		<div class="col-lg-offset-1 col-lg-10" id="layout_footer">
			<hr />
			<%=render 'layouts/footer'%>
		</div>
	</div>
</body>
</html>
